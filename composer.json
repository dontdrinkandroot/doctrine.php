{
    "name": "dontdrinkandroot/doctrine",
    "description": "Doctrine ORM Extensions",
    "keywords": [
        "php",
        "doctrine",
        "orm"
    ],
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Philip Washington Sorst",
            "email": "philip@sorst.net",
            "homepage": "https://sorst.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dontdrinkandroot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dontdrinkandroot\\": "test/"
        }
    },
    "require": {
        "php": ">=7.2",
        "psr/log": "~1.0",
        "doctrine/orm": "^2.5",
        "doctrine/common": "^2.7",
        "ramsey/uuid": "^3.8"
    },
    "require-dev": {
        "ext-pdo": "*",
        "squizlabs/php_codesniffer": "^3.0",
        "scrutinizer/ocular": "1.6.0",
        "doctrine/data-fixtures": "^1.1",
        "phpunit/phpunit": "^8.2",
        "symfony/yaml": "^4.0"
    },
    "suggest": {
        "doctrine/data-fixtures": "For data fixture testing"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
