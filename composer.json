{
    "name": "dontdrinkandroot/doctrine",
    "description": "Doctrine ORM Extensions",
    "keywords": [
        "php",
        "doctrine",
        "orm"
    ],
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Philip Washington Sorst",
            "email": "philip@sorst.net",
            "homepage": "https://sorst.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dontdrinkandroot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dontdrinkandroot\\": "test/"
        }
    },
    "require": {
        "php": ">=7.2",
        "psr/log": "~1.0",
        "doctrine/orm": "^2.5",
        "doctrine/common": "^2.7",
        "symfony/property-access": "^3.0|^4.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "phpunit/dbunit": "^4.0",
        "scrutinizer/ocular": "^1.1",
        "doctrine/data-fixtures": "^1.1"
    },
    "suggest": {
        "doctrine/data-fixtures": "For data fixture testing"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
